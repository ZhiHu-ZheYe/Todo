	<item name="android:ellipsize">marquee</item>
	<item name="android:layout_height">48dip</item>
	<item name="android:layout_width">fill_parent</item>
	<item name="android:paddingRight">10sp</item>
	<item name="android:singleLine">true</item>
	<item name="android:textAppearance">@style/TextAppearance.Widget.DropDownItem</item>
    </style>
    <style name="RepeatEditor.Button" parent="@android:style/Widget.Button.Small">
	<item name="android:background">@drawable/btn_radio_small</item>
	<item name="android:button">@color/invisible</item>
	<item name="android:layout_height">wrap_content</item>
	<item name="android:layout_weight">1</item>
	<item name="android:layout_width">wrap_content</item>
	<item name="android:gravity">center</item>
	<item name="android:textColor">@color/black</item>
	<item name="android:textAppearance">@style/TextAppearance.Small</item>
    </style>
    <style name="RepeatEditor.Button.Alternate">
	<item name="android:textSize">12sp</item>
    </style>
    <style name="RepeatEditor.Button.Interval">
	<item name="android:padding">2sp</item>
	<item name="android:textSize">12sp</item>
    </style>
    <style name="RepeatEditor.Button.Reset">
	<item name="android:textSize">14sp</item>
    </style>
    <style name="RepeatEditor.Button.Weekday">
	<item name="android:padding">2sp</item>
	<item name="android:textSize">14sp</item>
	<item name="android:width">24sp</item>
    </style>
    <style name="TextAppearance.Small" parent="@android:style/TextAppearance.Small">
	<item name="android:textSize">12sp</item>
